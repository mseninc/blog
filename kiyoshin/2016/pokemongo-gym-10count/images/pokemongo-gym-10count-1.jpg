/* pre タグに囲まれていない code タグ (インラインコード) のスタイル */
:not(pre)>code {
	border: solid 1px #ccd;
	padding: 0px .3em;
	border-radius: 3px;
}

/* class属性がついていない pre (Prism.js でスタイルが付与されない) */
pre:not(class) {
	/* Prism.js の CSS からコピー */
	color: #f8f8f2;
	background: 0 0;
	text-shadow: 0 1px rgb(0 0 0 / 30%);
	text-align: left;
	white-space: pre;
	word-spacing: normal;
	word-break: normal;
	word-wrap: normal;
	line-height: 1.5;
	-moz-tab-size: 4;
	-o-tab-size: 4;
	tab-size: 4;
	-webkit-hyphens: none;
	-moz-hyphens: none;
	-ms-hyphens: none;
	hyphens: none;
	/* Prism.js の okaidia テーマの背景色 */
	background: #272822;
	/* Prism.js と同様の外枠スタイル */
	padding: 1em;
	margin: 1em 0;
	margin-bottom: 1.5em;
	overflow: auto;
	border-radius: .3em;
}
pre[class*=language-] {
	margin-bottom: 1.5em;
}

